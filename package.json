{
  "name": "nextn",
  "version": "0.1.0",
  "private": true,
  "description": "Velocity Manager is a web-based application for managing game servers.",
  "author": "Velocity Manager Team",
  "main": "electron.js",
  "scripts": {
    "dev": "concurrently \"npm:dev:frontend\" \"npm:dev:backend\"",
    "dev:frontend": "next dev --turbopack -p 9002",
    "dev:backend": "node src/backend/src/index.js",
    "genkit:dev": "genkit start -- tsx src/ai/dev.ts",
    "genkit:watch": "genkit start -- tsx --watch src/ai/dev.ts",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "electron:dev": "concurrently -k \"npm:dev:backend\" \"npm:dev:frontend\" \"wait-on http://localhost:9002 && electron .\"",
    "preelectron:build": "npm run build",
    "electron:build": "electron-builder",
    "postinstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "@genkit-ai/googleai": "^1.8.0",
    "@genkit-ai/next": "^1.8.0",
    "@hookform/resolvers": "^4.1.3",
    "@iarna/toml": "^2.2.5",
    "@radix-ui/react-accordion": "^1.2.3",
    "@radix-ui/react-alert-dialog": "^1.1.6",
    "@radix-ui/react-avatar": "^1.1.3",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-menubar": "^1.1.6",
    "@radix-ui/react-popover": "^1.1.6",
    "@radix-ui/react-progress": "^1.1.2",
    "@radix-ui/react-radio-group": "^1.2.3",
    "@radix-ui/react-scroll-area": "^1.2.3",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-slider": "^1.2.3",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-switch": "^1.1.3",
    "@radix-ui/react-tabs": "^1.1.3",
    "@radix-ui/react-toast": "^1.2.6",
    "@radix-ui/react-tooltip": "^1.1.8",
    "ansi-to-html": "^0.7.2",
    "archiver": "^7.0.1",
    "body-parser": "^1.20.2",
    "call-bind": "^1.0.7",
    "call-bind-apply-helpers": "^1.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "date-fns": "^3.6.0",
    "dotenv": "^16.5.0",
    "express": "^4.19.2",
    "extract-zip": "^2.0.1",
    "firebase": "^11.9.1",
    "genkit": "^1.8.0",
    "lru-cache": "^11.1.0",
    "lucide-react": "^0.475.0",
    "multer": "^1.4.5-lts.1",
    "next": "15.3.3",
    "patch-package": "^8.0.0",
    "pidusage": "^4.0.1",
    "react": "^18.3.1",
    "react-day-picker": "^8.10.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.54.2",
    "recharts": "^2.15.1",
    "tailwind-merge": "^3.0.1",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^11.1.0",
    "wait-on": "^7.2.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/archiver": "^6.0.2",
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/iarna__toml": "^2.0.3",
    "@types/multer": "^1.4.11",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/uuid": "^9.0.8",
    "electron": "^31.2.1",
    "electron-builder": "^25.0.0",
    "genkit-cli": "^1.8.0",
    "postcss": "^8",
    "rimraf": "^5.0.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  },
  "build": {
    "appId": "com.velocitymanager.app",
    "productName": "VelocityManager",
    "copyright": "Copyright 2025 Velocity Manager Team",
    "files": [
      "public/**/*",
      ".next/**/*",
      "electron.js",
      "src/backend/src/**/*",
      "config.json",
      "next.config.ts"
    ],
    "directories": {
      "output": "release"
    },
    "win": {
      "target": "nsis"
    },
    "mac": {
      "target": "dmg"
    },
    "linux": {
      "target": "AppImage"
    }
  }
}
